package hibernate.one_to_one.service;

import java.util.List;

import hibernate.one_to_one.dao.QuestionAnsweDao;
import hibernate.one_to_one.dto.Answer;

public class QuestionAnswerDao {

	QuestionAnsweDao dao = new QuestionAnsweDao();
	
	public List<Answer> savequestionanswerservice(List<Answer> answers){
		return dao.savequestionanswerDao(answers);
}
}
